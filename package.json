{
  "name": "earnit",
  "version": "1.0.0",
  "description": "A fun app to burn that fast food",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
