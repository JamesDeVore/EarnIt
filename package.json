    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.4.19",
    "node-fetch": "^2.3.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "redux-logger": "^3.0.6",
    "validator": "^10.11.0"
  },
